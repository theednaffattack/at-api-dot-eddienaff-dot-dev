  "scripts": {
    "insertHotel": "ts-node -r dotenv/config ./src/scripts/seed-hotel-from-feature.ts",
    "insertVenues": "ts-node -r dotenv/config ./src/scripts/seed-venue-from-feature.ts",
    "insertUsers": "ts-node -r dotenv/config ./src/scripts/seed-users-faker.ts",
