    "typeorm:cli": "ts-node ./node_modules/.bin/typeorm --config src/config/prod-orm-migrations-config.ts",
    "typeorm:cli:d": "ts-node ./node_modules/typeorm/cli -f src/config/dev-orm-config.ts",
    "orm": "ts-node ./node_modules/.bin/typeorm --config src/config/seed-orm-config.ts",
    "orm:mig": "ts-node ./node_modules/.bin/typeorm --config src/config/prod-orm-migrations-config.ts migration:create -n",
    "orm:mig:gen": "ts-node ./node_modules/.bin/typeorm --config src/config/prod-orm-migrations-config.ts migration:generate -n",
    "orm:mig:revert": "ts-node ./node_modules/.bin/typeorm --config src/config/dev-orm-config.ts migration:revert",
