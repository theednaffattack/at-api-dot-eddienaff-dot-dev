    "build": "rimraf ./dist && tsc",
    "start": "npm run build && env NODE_ENV=production node -r dotenv/config ./dist/index.js",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
